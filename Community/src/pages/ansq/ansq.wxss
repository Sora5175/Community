view{
  background-color: white;
  width: 100%;
  display:flex;
}
#checkView{
  padding: 10rpx 0rpx;
  color: rgb(0, 0, 0);
}
#checkIndex{
  background-color: white;
  width: 716rpx;
  margin: 0px auto;
  border-radius: 11.7rpx;
  line-height: 70rpx;
  height: 70rpx;
}
#checkIndex>image{
  width: 45rpx;
  height: 45rpx;
  margin: 20rpx 4rpx 5rpx 20rpx;
}
#checkIndex>qname{
  width: 80%;
  display: inline-block;
  height: 70rpx;
  color: black;
}
#checkIndex.qname text{
    font-size: 32rpx;
    font-weight: 600;
    color: black;
}
  